    "!pip install ipython-sql sqlalchemy\n",
    "\n",
    "%load_ext sql\n",
    "\n",
    "%sql sqlite:///movieData.db\n",
    "\n",
    "import pandas as pd\n",
    "import sqlite3\n",
    "\n",
    "# Load the movie data\n",
    "moviedata = pd.read_csv('/kaggle/input/imdb-dataset-of-top-1000-movies-and-tv-shows/imdb_top_1000.csv')\n",
    "\n",
    "# Split the Genre column into multiple rows (handling multi-genre movies)\n",
    "moviedata_split = moviedata.assign(Genre=moviedata['Genre'].str.split(', ')).explode('Genre')\n",
    "\n",
    "# Connect to SQLite and save the processed data\n",
    "conn = sqlite3.connect('movieData.db')\n",
    "moviedata_split.to_sql('moviedata', conn, if_exists='replace', index=False)\n"
   ]
